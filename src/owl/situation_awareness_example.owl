    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#BreadKnife -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#BreadKnife">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Knife"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A device with jagged edges used to cut the bread.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#ButterKnife -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#ButterKnife">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Knife"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A device with no sharp edges used to spread butter on bread</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#Cucumber -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#Cucumber">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/situation_awareness_example.owl#Vegetable"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRole"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#AlteredObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical object that is eaten raw in the form of sticks, slices. It is used in salads.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#CuttingBoard -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#CuttingBoard">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#DesignedTool"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#Food -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#Food">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#BiologicalObject"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It includes the category of comestible objects</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#Sawing -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#Sawing">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#DirectedMotion"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A motion repeated to cut an object into its constituents. Especially, this motion is performed while cuttign bread with a knife containing jagged edges.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#Splitting -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#Splitting">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Variability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The disposition of an object (tool) to alter the shape of others in such a way that results in getting parts from the whole</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/situation_awareness_example.owl#Vegetable -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/situation_awareness_example.owl#Vegetable">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/situation_awareness_example.owl#Food"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An edible physical object obtained from plants</rdfs:comment>
    </owl:Class>
</rdf:RDF>



